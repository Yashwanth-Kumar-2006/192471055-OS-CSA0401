#include <stdio.h>
#include <string.h>
struct dir { char fname[10]; } d[10];
int main() {
    int n,i,flag; char f[10];
    printf("Enter number of files: "); scanf("%d",&n);
    for(i=0;i<n;i++) scanf("%s",d[i].fname);
    printf("Enter file to search: "); scanf("%s",f);
    flag=0;
    for(i=0;i<n;i++)
        if(strcmp(f,d[i].fname)==0){ printf("File found\n"); flag=1; break; }
    if(!flag) printf("File not found\n");
}
